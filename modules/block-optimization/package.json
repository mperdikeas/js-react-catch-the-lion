{
  "name": "ai-for-shogi-like-games",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
    "scripts": {
        "build": "babel es6 --out-dir es5 --source-maps",
        "prebuild__not_used": "for f in $(find es6/ -iname *.js | cut -c5-) ; do cp es6/$f es5/$f.flow; done",
    "build-watch": "babel es6 --out-dir es5 --source-maps --watch",
    "start": "node es5/index.js",
    "test": "mocha --compilers js:babel-core/register",
    "test-watch": "mocha --compilers js:babel-core/register --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.0",
    "babel-runtime": "^6.6.1",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "geometry-2d": "*",
    "lodash": "^4.10.0",
    "source-map-support": "^0.4.0"
  }
}
